{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "apk": {
      "android": {
        "buildType": "apk",
        "image": "expo:53"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_ANDROID_PROGUARD_ENABLED": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
